{% extends app.request.xmlHttpRequest
? 'Layout/ajaxLayout.html.twig'
: 'Layout/layout.html.twig'%}


{% block content %}
    {% include '@App/Components/navigation.html.twig' %}
    <div class="artist-list">
        {% for artist in artists %}
            <div class="card-col">
            {% include '@App/Partials/artist-card.html.twig' with {'artist': artist} only %}
            </div>
        {% endfor %}
    </div>
{% endblock %}